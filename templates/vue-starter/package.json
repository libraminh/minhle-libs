{
  "name": "vue-starter",
  "version": "1.0.0",
  "description": "Vue Boilerplate",
  "main": "app.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress ",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress "
  },
  "keywords": [
    "vue",
    "vue-router",
    "vue-boilerplate"
  ],
  "author": "Minh Le",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.3.3",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.3.1",
    <% if (includeEslint) { %>
    "babel-eslint": "10.0.1",
    <% } %>
    "autoprefixer": "9.4.7",
    <% if (includeAxios) { %>
    "axios": "0.18.0",
    <% } %>
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "1.0.1",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    <% if (includeEslint) { %>
    "eslint": "5.15.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-vue": "5.2.2",
    <% } %>
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "path": "0.12.7",
    "postcss-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "vue-loader": "15.6.2",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.6",
    "webpack": "4.29.4",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    <% if (includeFontAwesome) { %>
    "font-awesome": "4.7.0",
    <% } %>
    <% if (includeMomentJs) { %>
    "moment": "^2.24.0",
    <% } %>
    <% if (includeJquery) { %>
    "jquery": "3.3.1",
    <% } %>
    <% if (includeVueRouter) { %>
    "vue-router": "3.0.2",
    <% } %>
    <% if (includeVue2Animate) { %>
    "vue2-animate": "2.1.0",
    <% } %>
    <% if (includeVuex) { %>
    "vuex": "3.1.0",
    <% } %>
    "vue": "2.6.6"
  }
}
